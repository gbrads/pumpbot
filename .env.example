# RPC endpoint for Solana network interactions
PUMP_FUN_RPC=your_helius_rpc_endpoint_here

# Private key of the wallet used to fund new wallets (needs SOL)
FUNDING_WALLET_PRIVATE_KEY=your_funding_wallet_private_key_here

# Public key of the wallet to receive consolidated SOL from all wallets
BASE_WALLET_ADDRESS=your_base_wallet_public_key_here

# Private keys for token creation (first wallet creates, second wallet buys)
WALLET_A_PRIVATE_KEY=your_first_wallet_private_key_here  # Creator wallet
WALLET_B_PRIVATE_KEY=your_second_wallet_private_key_here # Initial buyer wallet

# Generated wallet private keys (automatically updated by fund script)
# WALLET_1_PRIVATE_KEY=key1
# WALLET_2_PRIVATE_KEY=key2
# etc...
